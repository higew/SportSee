{
    "source": {
        "include": ["src"], 
        "includePattern": "\\.(js)$",
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {                      
        "destination": "./JSdocs/",          
        "recurse": true
    }
}